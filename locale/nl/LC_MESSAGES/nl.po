msgid "Properties"
msgstr "Eigenschappen"

#: moredialogs.xrc:6170 moredialogs.xrc:7230 moredialogs.xrc:8367
msgid "General"
msgstr "Algemeen"

#: moredialogs.xrc:6214 moredialogs.xrc:7270 moredialogs.xrc:8406
msgid "Name:"
msgstr "Naam:"

#: moredialogs.xrc:6230 moredialogs.xrc:7286 moredialogs.xrc:8422
msgid "Location:"
msgstr "Locatie:"

#: moredialogs.xrc:6245 moredialogs.xrc:7301 moredialogs.xrc:8437
msgid "Type:"
msgstr "Type:"

#: moredialogs.xrc:6331 moredialogs.xrc:7445 moredialogs.xrc:8639
msgid "Size:"
msgstr "Grootte:"

#: moredialogs.xrc:6346 moredialogs.xrc:7460 moredialogs.xrc:8653
msgid "Accessed:"
msgstr "Gewijzigd:"

#: moredialogs.xrc:6360 moredialogs.xrc:7474 moredialogs.xrc:8667
msgid "Admin Changed:"
msgstr "Admin gewijzigd:"

#: moredialogs.xrc:6375 moredialogs.xrc:7489 moredialogs.xrc:8681
msgid "Modified:"
msgstr "Gewijzigd:"

#: moredialogs.xrc:6490 moredialogs.xrc:7619 moredialogs.xrc:8811
